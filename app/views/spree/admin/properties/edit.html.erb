<% content_for :page_title do %>
  <%= link_to Spree.t(:properties), admin_properties_url %> /
  <%= @property.name %>
<% end %>

<%= form_with model: [:admin, @property] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/resource_actions' %>
  </fieldset>
<% end %>
