<% content_for :page_title do %>
  <%= breadcrumb_builder(link_one_uri: spree.admin_properties_path, link_one_text: Spree.t(:all_properties), current_page_name: @property.name) %>
<% end %>

<%= form_with model: [:admin, @property] do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <%= render partial: 'spree/admin/shared/resource_actions' %>
<% end %>
