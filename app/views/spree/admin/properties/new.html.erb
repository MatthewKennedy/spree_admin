<% content_for :page_title do %>
  <%= breadcrumb_builder(link_one_uri: spree.admin_properties_path, link_one_text: Spree.t(:all_properties), current_page_name: Spree.t(:new_property)) %>
<% end %>

<%= form_with model: [:admin, @property] do |f| %>
  <fieldset data-hook="new_property">
    <%= render partial: 'form', locals: { f: f } %>
    <div class="form-actions">
      <%= render partial: 'spree/admin/shared/resource_actions' %>
    </div>
  </fieldset>
<% end %>
