<% content_for :page_title do %>
  <%= breadcrumb_builder(link_one_uri: spree.admin_shipping_methods_path, link_one_text: Spree.t(:shipping_methods), current_page_name: Spree.t(:new_shipping_method)) %>
<% end %>

<% content_for :page_actions do %>
  <%= remote_form_submit_button(@object) %>
<% end %>

<div data-hook="admin_shipping_method_new_form_header">
  <%= render partial: 'spree/admin/shared/error_messages', locals: { target: @shipping_method } %>
</div>

<div data-hook="admin_shipping_method_new_form">
  <%= form_with model: [:admin, @shipping_method], data: { controller: 'form-state'} do |f| %>
    <fieldset>
      <%= render partial: 'form', locals: { f: f } %>
    </fieldset>
  <% end %>
</div>
