<% content_for :page_title do %>
  <%= link_to Spree.t(:tax_categories), spree.admin_tax_categories_url %> /
  <%= Spree.t(:new_tax_category) %>
<% end %>

<% content_for :page_actions do %>
  <%= link_to_with_icon Spree.t(:create), new_object_url, class: "btn-success btn", icon: 'plus-square.svg', id: 'admin_new_tax_categories_link' %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @tax_category } %>

<%= form_with model: [:admin, @tax_category] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%#= render partial: 'spree/admin/shared/resource_actions' %>
  </fieldset>
<% end %>
