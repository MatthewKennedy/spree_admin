<% content_for :page_title do %>
  <%= breadcrumb_builder(link_one_uri: spree.admin_tax_rates_path, link_one_text: Spree.t(:tax_rates), current_page_name: Spree.t(:new_tax_rate)) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @tax_rate } %>

<%= form_with model: [:admin, @tax_rate], data: { controller: "input--required" } do |f| %>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/resource_actions' %>
<% end %>
