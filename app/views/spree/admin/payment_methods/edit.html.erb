<% content_for :page_title do %>
  <%= breadcrumb_builder(link_one_uri: spree.admin_payment_methods_path, link_one_text: Spree.t(:all_payment_methods), current_page_name: @payment_method.name) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @payment_method } %>

<%= form_with model: @payment_method, scope: :payment_method, url: admin_payment_method_path(@payment_method), data: {controller: "form-state"}, id: "mainForm" do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <%= render partial: 'spree/admin/shared/resource_actions' %>
<% end %>
