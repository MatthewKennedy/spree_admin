<% content_for :page_title do %>
  <%= link_to Spree.t(:payment_methods), spree.admin_payment_methods_path %> / <%= Spree.t(:new_payment_method) %>
<% end %>

<% content_for :page_actions do %>
  <%= remote_form_submit_button(@object) %>
<% end %>

<%= form_with model: @payment_method, url: collection_url, id: "mainForm" do |f| %>
    <%= render partial: 'form', locals: { f: f } %>
<% end %>
