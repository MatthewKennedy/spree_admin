<% content_for :page_title do %>
  <%= breadcrumb_builder(link_one_uri: spree.admin_stock_locations_path, link_one_text: Spree.t(:stock_locations),
                         link_two_uri: spree.admin_stock_location_stock_movements_path(stock_location), link_two_text: Spree.t(:stock_movements),
                           current_page_name: Spree.t(:new_stock_movement)) %>
<% end %>

<%= form_with model: [:admin, stock_location, @stock_movement] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/resource_actions', locals: { collection_url: admin_stock_location_stock_movements_path(stock_location) } %>
  </fieldset>
<% end %>
