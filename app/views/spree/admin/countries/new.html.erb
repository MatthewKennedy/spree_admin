<% content_for :page_title do %>
  <%= link_to Spree.t(:countries), spree.admin_countries_url %> / <%= Spree.t(:new_country) %>
<% end %>

<% content_for :page_actions do %>
  <%= remote_form_submit_button(@country) %>
<% end %>

<%= form_with model: [:admin, @country], builder: Spree::Dash::BootstrapBuilder, id: "mainForm" do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
<% end %>
