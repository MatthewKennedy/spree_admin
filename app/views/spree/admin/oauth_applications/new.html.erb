<% content_for :page_title do %>
  <%= link_to I18n.t('spree.dash.oauth_applications.list'), spree.admin_oauth_applications_url %> /
  <%= I18n.t('spree.dash.oauth_applications.new') %>
<% end %>

<% content_for :page_actions do %>
  <%= remote_form_submit_button(@object) %>
<% end %>

<%= turbo_frame_tag 'new_oauth_application' do %>
  <%= form_with model: [:admin, @oauth_application] do |f| %>
      <%= render partial: 'form', locals: { f: f } %>
      <%= render partial: 'spree/admin/shared/new_resource_links' %>
  <% end %>
<% end %>
